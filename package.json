{
  "name": "next-image-cache-cleaner",
  "version": "1.0.0",
  "description": "A utility for resolving high memory usage issues in the Next.js Image Optimizer module on Linux.",
  "main": "dist/next-image-cache-cleaner.cjs",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "coverage": "vitest run --coverage",
    "coverage-ui": "vitest --ui",
    "format": "prettier . --check",
    "format:fix": "prettier --write .",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "start": "node dist/next-image-cache-cleaner.cjs",
    "test": "vitest",
    "ts-check": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VadimOnix/next-image-cache-cleaner.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VadimOnix/next-image-cache-cleaner/issues"
  },
  "homepage": "https://github.com/VadimOnix/next-image-cache-cleaner#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "^9.20.0",
    "@types/node": "^22.13.1",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.5",
    "eslint": "^9.20.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "nodemon": "^3.1.9",
    "pino-pretty": "^13.0.0",
    "prettier": "3.4.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.23.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "chokidar": "^4.0.3",
    "dotenv": "^16.4.7",
    "pino": "^9.6.0"
  }
}
